$(function(){
	
	let listForm = $('#listForm');
	$('.title').on('click', function(e) {
		e.preventDefault();
		$('[name="ano"]').remove();
		let bnoData = "<input type='hidden' name='ano' value='" + $(this).attr('href') + "'/>";
		listForm.append(bnoData)
			.attr("action", `${contextPath}/album/detail`)
			.submit();
	});
	
});

